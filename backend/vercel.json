{
  "installCommand": "npm install --  legacy-peer-deps",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "src/**/*",
      "use":"@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)", "dest":"backend/server.js"}
    ]
}